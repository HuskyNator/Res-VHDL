{
    "repository": {"url": "https://github.com/HuskyNator/Res-VHDL/tree/ANS/ANS"},
    "name": "asn",
    "displayName": "ASN",
    "description": "Adrian Syntax Notation Highlighting",
    "version": "0.0.2",
    "icon": "images/icon.png",
    "keywords": ["asn", "metasyntax-notation"],
    "author": {"name": "HuskyNator"},
    "license": "ISC",
    "engines": {"vscode": "^1.63.0"},
    "categories": ["Programming Languages"],
    "contributes": {
        "markdown.markdownItPlugins": true,
        "languages": [
            {
                "id": "asn",
                "aliases": ["ASN"],
                "extensions": ["asn"],
                "configuration": "./language-configuration.json"
            },{
                "id": "asn-injection",
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "asn",
                "scopeName": "source.asn",
                "path": "./syntaxes/asn.tmLanguage.json"
            },{
                "language": "asn-injection",
                "scopeName": "asn.injection",
                "path": "./syntaxes/asn-injection.tmLanguage.json",
                "injectTo": ["text.html.markdown"],
                "embeddedLanguages": {
                    "asn.injection.code": "asn"
                }
            }
        ]
    },
    "main": "./out/extension.js",
    "scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	}
}